{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ],
    "@babel/typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/transform-runtime"],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
